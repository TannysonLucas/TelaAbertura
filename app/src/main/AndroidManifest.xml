<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tannyson.telaabertura">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_imagem"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_imagem"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Teste"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"
                android:label="@string/app_name"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TelaAbertura"
                  android:label="@string/app_layout2">
            <intent-filter>
                <action android:name="com.example.tannyson.telaabertura.TelaAbertura" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>